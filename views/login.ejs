
<% include partials/header%>
  <!--start of navbar-->
  <nav id ="navbar" class="navbar fixed-top navbar-dark bg-dark">
  <div class="container">
  <a class="navbar-brand" href="/">
    <img id="navbar-icon" class="hide" src="images/mov_LuckyDolphin_Logo.png" width="90" height="50"  alt="">
  </a>
  <button id="loginbtn" class="btn btn-sm align-right btn-outline-secondary hide" type="button"><i class="fa fa-sign-in" aria-hidden="true"></i> Login</button>
  </div>
  </nav> 
  <!--end of navbar-->
  <div id='flashMassege' class='container'>
      <% if(success && success.length > 0){%>
      <div class="alert alert-success" role="alert">
           <%=success%>
      </div>
      <% } %>
      <% if(error && error.length > 0){%>
      <div class="alert alert-danger" role="alert">
           <%=error%>
      </div>
      <% } %>
  </div>
  <!--start of intro div-->

  <div id="intro-div" class="hide">
   <div class="main-con container">
    <div class="jumbotron">
     <h1>Lucky Dolphin</h1>
     <h2>Speak Friend and Enter</h2>
    </div>
   </div>
  </div>
  
  

  <!--end of intro div-->

  <!--start of login div-->

  <div id="login-div" class="hide">
  <div class="main-con container">
   <div class="jumbotron">
    <form  action="/login" method="post">
    <div class="form-group">
       <label for="uname1">Username</label>
       <input type="text" class="form-control" name="username" id="uname1" required placeholder="Username">
    </div>
    <div class="form-group">
      <label>Password</label>
      <input type="password" class="form-control" name="password" id="pwd1" required autocomplete="new-password" placeholder="Password">
    </div>
    <button id="submitbtn" type="submit" class="btn btn-default"><i class="fa fa-sign-in" aria-hidden="true"></i> Login</button>
    </form>
   </div>
  </div>
 </div>

  <!--end of login div-->
<div class='hide' id='snackbar'>Some text here...</div>
 <% include partials/footer%>